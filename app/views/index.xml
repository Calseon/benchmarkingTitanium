<Alloy>
	<Window class="container" layout="vertical">
		<Label id="lblTime">Time:</Label>
		<TextField id="tf_numViews" height="Titanium.UI.SIZE" width="Titanium.UI.FILL" keyboardType="Ti.UI.KEYBOARD_NUMBER_PAD"> </TextField>
		<View layout="horizontal" height="Titanium.UI.SIZE" width="Titanium.UI.FILL">
			<Button height="Titanium.UI.SIZE" title="Start" onClick="startTest"></Button>
			<Button height="Titanium.UI.SIZE" title="Reset" onClick="resetTest"></Button>
		</View>
		<ScrollView layout="vertical" height="Titanium.UI.SIZE" width="Titanium.UI.FILL" id="viewGenContainer">
			
		</ScrollView>
	</Window>
</Alloy>